<?xml version="1.0" encoding="ISO-8859-1"?>
<template type="ProjTempl" version="1.0" revision="1.0"
		id="EMSK_EM11D_HelloWorld_CPP_Project" label="%template.label" description="%template.description">

	<property-group id="basics" label="%basics.label" description="%basics.description" type="PAGES-ONLY" help="help.html">
		<property id="author"
			label="%author.label"
			description="%author.description"
			type="input"
			pattern=".*"
			default=""
			hidden="false"
			persist="true"/>
		<property id="copyright"
			label="%copyright.label"
			description="%copyright.description"
			type="input"
			pattern=".*"
			default="%copyright.default"
			hidden="false"
			persist="true"/>
		<property id="messagearc"
			label="%message.label"
			description="%message.description"
			type="input"
			pattern=".*"
			default="%message.default"
			hidden="false"
			persist="true"/>
		<property id="sourceDir"
				label="%sourceDir.label"
				description="%sourceDir.description"
				type="input"
				default="%sourceDir.default"
				pattern="[a-zA-Z0-9]+"
				mandatory="false"
				persist="true"/>
	</property-group>

	<process type="org.eclipse.cdt.managedbuilder.core.NewManagedProject">
		<simple name="name" value="$(projectName)" /> 
		<simple name="artifactExtension" value="elf" /> 
		<simple name="isCProject" value="true" /> 
	</process>

	<process type="org.eclipse.cdt.core.CreateSourceFolder">
		<simple name="projectName" value="$(projectName)"/>
		<simple name="path" value="$(sourceDir)"/>
	</process>

        <process type="org.eclipse.cdt.core.AddFiles">
                <simple name="projectName" value="$(projectName)"/>
                <complex-array name="files">
			<element>
				<simple name="source" value="src/Basename.cpp"/>
				<simple name="target" value="$(sourceDir)/$(projectName).cpp"/>
				<simple name="replaceable" value="true"/>
			</element>
                </complex-array>
        </process>

	<process type="org.eclipse.cdt.ui.OpenFiles">
		<simple name="projectName" value="$(projectName)"/>
		<complex-array name="files">
			<element>
				<simple name="target" value="$(sourceDir)/$(projectName).cpp"/>
			</element>
		</complex-array>
	</process>

	<process type="org.eclipse.cdt.managedbuilder.core.SetMBSStringOptionValue">
		<simple name="projectName" value="$(projectName)"/>
		<complex-array name="resourcePaths">
			<element>
				<simple name="id" value="org.eclipse.cdt.cross.arc.gnu.linux.option.target.processor" />
				<simple name="value" value="org.eclipse.cdt.cross.arc.gnu.base.option.mcpu.em4_fpuda" />
				<simple name="path" value="" />
			</element>
			<element>
				<simple name="id" value="org.eclipse.cdt.cross.arc.gnu.linux.option.target.mpyem" />
				<simple name="value" value="org.eclipse.cdt.cross.arc.gnu.base.option.mpyem.wlh1" />
				<simple name="path" value="" />
			</element>
			<element>
				<simple name="id" value="org.eclipse.cdt.cross.arc.gnu.linux.option.target.fpuem" />
				<simple name="value" value="org.eclipse.cdt.cross.arc.gnu.base.option.fpu.fpuda_all" />
				<simple name="path" value="" />
			</element>
			<element>
				<simple name="id" value="org.eclipse.cdt.cross.arc.gnu.em.linux.cpp.linker.base.option.specs" />
				<simple name="value" value="org.eclipse.cdt.cross.arc.gnu.base.option.linker.specsem.emsk_em11d" />
				<simple name="path" value="" />
			</element>
		</complex-array>
	</process>

	<process type="org.eclipse.cdt.managedbuilder.core.SetMBSBooleanOptionValue">
		<simple name="projectName" value="$(projectName)"/>
		<complex-array name="resourcePaths">
			<element>
				<simple name="id" value="org.eclipse.cdt.cross.arc.gnu.eb.linux.option.target.barrelshifter" />
				<simple name="value" value="true" />
				<simple name="path" value="" />
			</element>
			<element>
				<simple name="id" value="org.eclipse.cdt.cross.arc.gnu.eb.linux.option.target.divide" />
				<simple name="value" value="true" />
				<simple name="path" value="" />
			</element>
			<element>
				<simple name="id" value="org.eclipse.cdt.cross.arc.gnu.eb.linux.option.target.normalize" />
				<simple name="value" value="true" />
				<simple name="path" value="" />
			</element>
			<element>
				<simple name="id" value="org.eclipse.cdt.cross.arc.gnu.eb.linux.option.target.swap" />
				<simple name="value" value="true" />
				<simple name="path" value="" />
			</element>
			<element>
				<simple name="id" value="org.eclipse.cdt.cross.arc.gnu.linux.option.target.barrelshifter" />
				<simple name="value" value="true" />
				<simple name="path" value="" />
			</element>
			<element>
				<simple name="id" value="org.eclipse.cdt.cross.arc.gnu.linux.option.target.divide" />
				<simple name="value" value="true" />
				<simple name="path" value="" />
			</element>
			<element>
				<simple name="id" value="org.eclipse.cdt.cross.arc.gnu.linux.option.target.normalize" />
				<simple name="value" value="true" />
				<simple name="path" value="" />
			</element>
			<element>
				<simple name="id" value="org.eclipse.cdt.cross.arc.gnu.linux.option.target.swap" />
				<simple name="value" value="true" />
				<simple name="path" value="" />
			</element>
			<element>
				<simple name="id" value="org.eclipse.cdt.cross.arc.gnu.eb.linux.option.target.codedensity" />
				<simple name="value" value="true" />
				<simple name="path" value="" />
			</element>
			<element>
				<simple name="id" value="org.eclipse.cdt.cross.arc.gnu.linux.option.target.codedensity" />
				<simple name="value" value="true" />
				<simple name="path" value="" />
			</element>
		</complex-array>
	</process>
</template>

