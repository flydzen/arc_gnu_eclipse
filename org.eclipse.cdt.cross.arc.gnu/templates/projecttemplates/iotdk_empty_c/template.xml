<?xml version="1.0"?>
<template type="ProjTempl"
  id="iotdk_empty_c_project"
  label="%template.label"
  description="%template.description"
  version="1" revision="2" supplier="Synopsys">

  <process type="org.eclipse.cdt.managedbuilder.core.NewManagedProject">
    <simple name="name" value="$(projectName)" />
    <simple name="artifactExtension" value="elf" />
    <simple name="isCProject" value="true" />
  </process>

  <process type="org.eclipse.cdt.core.AddFiles">
    <simple name="projectName" value="$(projectName)" />
    <complex-array name="files">
      <element>
        <simple name="source" value="arcem9d.tcf" />
        <simple name="target" value="arcem9d.tcf" />
        <simple name="replaceable" value="true" />
      </element>
    </complex-array>
  </process>

  <process type="org.eclipse.cdt.core.CreateSourceFolder">
    <simple name="projectName" value="$(projectName)" />
    <simple name="path" value="src" />
  </process>

  <process type="org.eclipse.cdt.managedbuilder.core.SetMBSStringOptionValue">
    <simple name="projectName" value="$(projectName)" />
    <complex-array name="resourcePaths">
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.cpuem" />
        <simple name="value" value="com.synopsys.arc.gnu.elf.toolchain.base.target.cpuem.em4_fpuda" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.mpyem" />
        <simple name="value" value="com.synopsys.arc.gnu.elf.toolchain.base.target.mpyem.wlh1" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.fpuem" />
        <simple name="value" value="com.synopsys.arc.gnu.elf.toolchain.base.target.fpuem.fpuda_all" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="org.eclipse.cdt.cross.arc.gnu.em.linux.c.linker.base.option.specs" />
        <simple name="value" value="org.eclipse.cdt.cross.arc.gnu.base.option.linker.specsem.nosys" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.tcf_path" />
        <simple name="value" value="${workspace_loc:/$(projectName)/arcem9d.tcf}" />
        <simple name="path" value="" />
      </element>
    </complex-array>
  </process>

  <process type="org.eclipse.cdt.managedbuilder.core.AppendToMBSStringListOptionValues">
    <simple name="projectName" value="$(projectName)" />
    <complex-array name="resourcePaths">
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.tool.c.linker.arc.other.ldflags" />
        <simple-array name="values">
          <element value="--defsym=ivtbase_addr=0x0" />
        </simple-array>
        <simple name="path" value="" />
      </element>
    </complex-array>
  </process>

  <process type="org.eclipse.cdt.managedbuilder.core.SetMBSBooleanOptionValue">
    <simple name="projectName" value="$(projectName)" />
    <complex-array name="resourcePaths">
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.barrelshifter" />
        <simple name="value" value="true" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.divrem" />
        <simple name="value" value="true" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.norm" />
        <simple name="value" value="true" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.swap" />
        <simple name="value" value="true" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.codedensity" />
        <simple name="value" value="true" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.use_tcf" />
        <simple name="value" value="true" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.tcf_map" />
        <simple name="value" value="true" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.tcf_cinclude" />
        <simple name="value" value="true" />
        <simple name="path" value="" />
      </element>
    </complex-array>
  </process>
</template>
