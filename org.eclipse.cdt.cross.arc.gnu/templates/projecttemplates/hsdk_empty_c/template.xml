<?xml version="1.0"?>
<template type="ProjTempl"
  id="hsdk_empty_c_project"
  label="%template.label"
  description="%template.description"
  version="1" revision="2" supplier="Synopsys">

  <process type="org.eclipse.cdt.managedbuilder.core.NewManagedProject">
    <simple name="name" value="$(projectName)" />
    <simple name="artifactExtension" value="elf" />
    <simple name="isCProject" value="true" />
  </process>

  <process type="org.eclipse.cdt.core.AddFiles">
    <simple name="projectName" value="$(projectName)" />
    <complex-array name="files">
      <element>
        <simple name="source" value="archs38_c1.tcf" />
        <simple name="target" value="archs38_c1.tcf" />
        <simple name="replaceable" value="true" />
      </element>
    </complex-array>
  </process>

  <process type="org.eclipse.cdt.core.CreateSourceFolder">
    <simple name="projectName" value="$(projectName)" />
    <simple name="path" value="src" />
  </process>

  <process type="org.eclipse.cdt.managedbuilder.core.SetMBSStringOptionValue">
    <simple name="projectName" value="$(projectName)" />
    <complex-array name="resourcePaths">
      <element>
        <simple name="id" value="org.eclipse.cdt.cross.arc.gnu.hs.linux.option.target.processor" />
        <simple name="value" value="org.eclipse.cdt.cross.arc.gnu.base.option.mcpu.hs38_linux" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.mpyhs" />
        <simple name="value" value="com.synopsys.arc.gnu.elf.toolchain.base.target.mpyhs.plus_qmacw" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.fpuhs" />
        <simple name="value" value="com.synopsys.arc.gnu.elf.toolchain.base.target.fpuhs.fpud_all" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="org.eclipse.cdt.cross.arc.gnu.hs.linux.c.linker.base.option.specs" />
        <simple name="value" value="org.eclipse.cdt.cross.arc.gnu.base.option.linker.specs.nosys" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.tcf_path" />
        <simple name="value" value="${workspace_loc:/$(projectName)/archs38_c1.tcf}" />
        <simple name="path" value="" />
      </element>
    </complex-array>
  </process>

  <process type="org.eclipse.cdt.managedbuilder.core.AppendToMBSStringListOptionValues">
    <simple name="projectName" value="$(projectName)" />
    <complex-array name="resourcePaths">
      <element>
        <simple name="id" value="org.eclipse.cdt.cross.arc.gnu.c.link.option.ldflags" />
        <simple-array name="values">
          <element value="--defsym=ivtbase_addr=0x0" />
        </simple-array>
        <simple name="path" value="" />
      </element>
    </complex-array>
  </process>

  <process type="org.eclipse.cdt.managedbuilder.core.SetMBSBooleanOptionValue">
    <simple name="projectName" value="$(projectName)" />
    <complex-array name="resourcePaths">
      <element>
        <simple name="id" value="org.eclipse.cdt.cross.arc.gnu.hs.linux.option.target.divide" />
        <simple name="value" value="true" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.ll64" />
        <simple name="value" value="true" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.use_tcf" />
        <simple name="value" value="true" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.tcf_map" />
        <simple name="value" value="true" />
        <simple name="path" value="" />
      </element>
      <element>
        <simple name="id" value="com.synopsys.arc.gnu.elf.toolchain.base.target.tcf_cinclude" />
        <simple name="value" value="true" />
        <simple name="path" value="" />
      </element>
    </complex-array>
  </process>
</template>
